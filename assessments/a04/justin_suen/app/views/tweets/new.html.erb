<h1>New Tweet</h1>

<form action="/tweets" method="post">
  <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">

  <label for="title">Title
  <input type="text" name="tweet[title]" value="<%= @tweet.title %>">
  </label>
  <br>

  <label for="body">Body
  <input type="text" name="tweet[body]" value="<%= @tweet.body %>">
  </label>
  <br>

  <input type="submit" name="submit" value="Create New Tweet">
</form>
